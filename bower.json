{
  "name": "ulink-version-2",
  "version": "2.0.0",
  "homepage": "https://github.com/ulinkinc/ulink-version-2",
  "authors": [
    "Bennie Kingwood <bennie.kingwood@gmail.com>"
  ],
  "description": "Find out what is happening on your campus, right now.",
  "license": "MIT",
  "private": true,
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ], 
  "directory": "dependencies", 
  "dependencies": {
    "jquery": "~2.0.0", 
    "ember": "1.3.0",
		"handlebars": "1.3.0",
    "bootstrap": "~3.0.2", 
    "modernizr": "~2.7.1", 
    "bowser": "latest"
  },
	"exportsOverride": {
		"bootstrap": {
			"js" : ["dist/js/*.min.js"], 
			"css": ["dist/css/bootstrap.min.css"]
		},
		"handlebars": {
			"js" : ["handlebars.runtime.min.js"]
		},
		"ember": {
			"js" : ["ember.prod.js"]
		},
		"jquery": {
			"js" : ["jquery.min.js"]
		},
		"modernizr": {
			"js" : ["modernizr-1.7.min.js"]
		},
		"*": {                  
      "js": "**/*.min.js"
    }
	}
}